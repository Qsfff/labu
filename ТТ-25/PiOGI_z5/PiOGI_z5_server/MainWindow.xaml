<Window x:Class="PiOGI_z5_server.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PiOGI_z5_server"
        mc:Ignorable="d"
        Title="MainWindow" Height="468" Width="800">
    <Grid>
        <Label Content="Log" HorizontalAlignment="Left" Height="28" Margin="22,10,0,0" VerticalAlignment="Top" Width="432"/>
        <ListBox x:Name="log" HorizontalAlignment="Left" Height="318" Margin="17,38,0,0" VerticalAlignment="Top" Width="760" d:ItemsSource="{d:SampleData ItemCount=5}"/>
        <Button Content="Start server" HorizontalAlignment="Left" Height="57" Margin="25,367,0,0" VerticalAlignment="Top" Width="361" Background="#FFFFF5F5" FontSize="36" Click="strat"/>
        <Button Content="Stop server" HorizontalAlignment="Left" Height="57" Margin="400,367,0,0" VerticalAlignment="Top" Width="377" Background="Red" FontSize="36" Click="sotp"/>

    </Grid>
</Window>
